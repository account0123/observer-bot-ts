{
  "name": "observer-bot",
  "version": "1.0.0",
  "description": "Discord bot written in Typescript and discord.js hosted on Heroku",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prestart:dev": "npm run build",
    "start:dev": "nodemon build/index.js",
    "prebuild": "rimraf ./build",
    "build": "npx tsc",
    "postbuild": "copyfiles -u 1 \"./src/**/*.json\" ./build/",
    "prestart": "npm run build",
    "start": "node build/index.js",
    "test": "node build/index.js --beta",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/account0123/observer-bot-ts.git"
  },
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "heroku"
  ],
  "author": "Aman Shaikh (mxiv)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/account0123/observer-bot-ts/issues"
  },
  "homepage": "https://github.com/account0123/observer-bot-ts",
  "devDependencies": {
    "@types/node": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.4.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@types/aes-js": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/mysql": "^2.15.15",
    "aes-js": "^3.1.2",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-search-results-nodejs": "^2.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "npx": "^10.2.2",
    "rae-api": "^1.2.5",
    "rimraf": "^3.0.2"
  }
}
